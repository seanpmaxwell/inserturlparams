{
  "name": "inserturlparams",
  "version": "2.0.3",
  "description": "Insert dynamic data into url path params",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && rollup -c rollup.config.mjs",
    "test": "node --experimental-specifier-resolution=node --loader ts-node/esm test/index.ts"
  },
  "exports": {
    "require": "./dist/cjs/index.cjs",
    "import": "./dist/esm/index.js",
    "default": "./dist/cjs/index.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seanpmaxwell/inserturlparams.git"
  },
  "keywords": [
    "insert",
    "url",
    "param",
    "params",
    "dynamic",
    "setup",
    "node",
    "client",
    "browser"
  ],
  "author": "sean maxwell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seanpmaxwell/inserturlparams/issues"
  },
  "homepage": "https://github.com/seanpmaxwell/inserturlparams#readme",
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "rimraf": "^6.0.1",
    "rollup": "^4.19.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4"
  }
}
